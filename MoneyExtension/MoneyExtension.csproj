    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  
    <PackageId>BeritMoneyExtensions</PackageId>
    <Version>1.0.1</Version>
    <License>MIT</License>
    <Authors>Berit Carvalho</Authors>
    <Company>beri.dev</Company>
